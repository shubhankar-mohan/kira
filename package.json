{
  "name": "kirana-task-manager",
  "version": "1.0.0",
  "description": "KiranaClub Task Management System with Google Sheets Integration",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "prisma:generate": "prisma generate --schema=backend/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy --schema=backend/prisma/schema.prisma",
    "prisma:seed": "node backend/prisma/seed.js",
    "clean": "rm -rf node_modules/.prisma && npm run prisma:generate",
    "fix-permissions": "chmod -R 755 node_modules 2>/dev/null || true",
    "setup": "./start.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "task-management",
    "google-sheets",
    "nodejs",
    "express",
    "kirana-club"
  ],
  "author": "KiranaClub Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@slack/bolt": "^4.4.0",
    "@slack/web-api": "^7.9.3",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.0.1",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^5.0.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.0",
    "node-cron": "^4.2.1",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "prisma": "^5.22.0",
    "nodemon": "^3.1.4"
  }
}
